{
  "buffer_font_family": "FiraCode Nerd Font",
  "ui_font_size": 14,
  "buffer_font_size": 14,
  "minimap": {
    "show": "auto"
  },
  "telemetry": {
    "metrics": false,
    "diagnostics": false
  },
  "vim_mode": true,
  "theme": {
    "mode": "system",
    "light": "Catppuccin Latte",
    "dark": "Catppuccin Mocha"
  },
  "icon_theme": "Material Icon Theme",
  "auto_install_extensions": {
    "catppuccin": true,
    "docker-compose": true,
    "dockerfile": true,
    "git-firefly": true,
    "html": true,
    "live-server": true,
    "material-icon-theme": true,
    "nix": true
  }
}
