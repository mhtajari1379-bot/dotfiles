{
  "buffer_font_family": "Iosevka",
  "buffer_font_weight": 300.0,
  "buffer_font_size": 14,
  "ui_font_family": "Iosevka",
  "ui_font_weight": 300.0,
  "ui_font_size": 14,
  "cursor_blink": false,
  "current_line_highlight": "none",
  "scrollbar": {
    "show": "never"
  },
  "minimap": {
    "show": "always"
  },
  "tabs": {
    "file_icons": true
  },
  "telemetry": {
    "metrics": false,
    "diagnostics": false
  },
  "vim_mode": true,
  "theme": {
    "mode": "system",
    "light": "Tokyo Night Light",
    "dark": "Tokyo Night"
  },
  "icon_theme": {
    "mode": "system",
    "light": "Material Icon Theme",
    "dark": "Material Icon Theme"
  },
  "auto_install_extensions": {
    "docker-compose": true,
    "dockerfile": true,
    "fish": true,
    "git-firefly": true,
    "html": true,
    "live-server": true,
    "make": true,
    "material-icon-theme": true,
    "tokyo-night": true
  }
}
